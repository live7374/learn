1.
n1 = input()
n2 = input()
n3 = input()

sum = int(n1)+int(n2)+int(n3)
mult = int(n1)*int(n2)*int(n3)
other = int(n1)**(int(n2)/int(n3))
print("sum:", sum, "mult:", mult, "other:", other)

